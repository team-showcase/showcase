      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "tags": {
      "keywords": []
    },
    "sourceTemplate": {
      "id": "servicecatalog.connectivityComponentForManifest",
      "version": "0.0.0"
    },
    "dataSources": {
      "ZGBLMTORDER_SRV": {
        "uri": "sap/opu/odata/sap/ZGBLMTORDER_SRV/",
