      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "showcase.ZGBLM001.i18n.i18n"
        }
      },
      "ZGBLMTORDER_SRV": {
        "dataSource": "ZGBLMTORDER_SRV",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "showcase.ZGBLM001.view",
				"controlAggregation": "",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed": {
					"target": [
						"Master",
						"NotFound"
					]
				}
			},
			"routes": [
				{
					"name": "Master",
					"pattern": "",
					"target": [
						"Master"
					],
					"greedy": false
				},
				{
					"name": "Detail",
					"pattern": "Detail/{masterId},{orderNo}",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"Master",
						"Detail"
					]
				},
				{
					"name": "Create",
					"pattern": "Create{masterId}",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"Master",
						"Create"
					]
				},
				{
					"name": "NotFound",
					"pattern": "NouFound",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"Master",
						"NotFound"
					]
				}
			],
			"targets": {
				"Master": {
					"viewType": "XML",
					"viewName": "S002",
					"viewLevel": 1,
					"controlAggregation": "masterPages"
				},
				"Detail": {
					"viewType": "XML",
					"viewName": "S003",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"Create": {
					"viewType": "XML",
					"viewName": "S004",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"NotFound": {
					"viewType": "XML",
					"viewName": "S005",
					"controlAggregation": "detailPages",
					"viewLevel": 2
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
