                "title": "{{appTitle}}",
                "info": "{{appTitle}}",
                "subTitle": "{{appSubTitle}}",
                "icon": "sap-icon://Chart-Tree-Map"
            }
        }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
        "sap_hcb",
        "sap_belize"
